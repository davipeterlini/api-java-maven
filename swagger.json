    "contact": {
      "name": "Example - API with Java 11 and Maven",
      "url": "https://github.com/davipeterlini/api-java-maven",
      "email": "davipeterlini@gmail.com"
    },
    "version": "1;2022-02-28"
  },
  "servers": [
    {
      "url": "http://localhost:8091/api/",
      "description": "localhost"
    },
    {
      "url": "http://DNS:PORT/BASE-PATH/",
      "description": "ENVIRONMENT"
    }
  ],
  "tags": [
    {
      "name": "Resources",
      "description": "\"Este Controller tem como objetivo exibir uma saída de texto (hello wolrd) para teste da API "
    },
    {
      "name": "hello-world-controller",
      "description": "Hello World Controller"
    }
  ],
  "paths": {
    "/api/hello-world": {
      "get": {
        "tags": [
          "hello-world-controller"
        ],
        "summary": "getHelloWorldInRest",
        "operationId": "getHelloWorldInRestUsingGET",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/HelloWorldDTO"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "security": [
          {
            "x-client-key": [
              "global"
            ]
          }
        ]
      }
    },
    "/domains/v1/api-java-maven/api/hello-world": {
      "get": {
        "tags": [
          "hello-world-controller"
        ],
        "summary": "getHelloWorldInRest",
        "operationId": "getHelloWorldInRestUsingGET_1",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/HelloWorldDTO"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "security": [
          {
            "x-client-key": [
              "global"
            ]
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "HelloWorldDTO": {
        "title": "HelloWorldDTO",
        "type": "object",
        "properties": {
          "messages": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "x-client-key": {
        "type": "apiKey",
        "name": "x-client-key",
        "in": "header"
      }
    }
  }
}